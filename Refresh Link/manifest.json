{
  "manifest_version": 3,
  "name": "Multi URL Auto Refresh",
  "version": "1.0",
  "description": "Add multiple URLs and auto-refresh each at its own interval.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
